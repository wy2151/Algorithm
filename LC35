class Solution:
    def searchInsert(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        first = 0
        last = len(nums) - 1
        found = False
        
        while first<=last and not found:
            midpoint = (first + last)//2
            if target == nums[midpoint]:
                found = True
                return midpoint
            else:
                if target < nums[midpoint]:
                    last = midpoint - 1
                else:
                    first = midpoint + 1
        else:
            newnums = nums.append(target)
            newnums = newnums.sort()
            return newnums.loc[target]
##Question: 'newnums' output is null!
		
